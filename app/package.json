{
  "name": "app",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "node scripts/dev.mjs",
    "dev:frontend": "vite",
    "dev:test": "TAURI_E2E=true vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "tauri": "tauri",
    "sidecar:install": "cd sidecar && npm install",
    "sidecar:build": "cd sidecar && npm ci && npm run build",
    "postinstall": "cd sidecar && npm install",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:unit": "vitest run src/__tests__/stores/ src/__tests__/lib/ src/__tests__/hooks/",
    "test:integration": "vitest run src/__tests__/components/ src/__tests__/pages/",
    "test:e2e": "playwright test",
    "test:e2e:dashboard": "playwright test --grep @dashboard",
    "test:e2e:settings": "playwright test --grep @settings",
    "test:e2e:workflow": "playwright test --grep '@workflow|@workflow-agent'",
    "test:e2e:navigation": "playwright test --grep @navigation",
    "test:e2e:skills": "playwright test --grep @skills",
    "test:e2e:usage": "playwright test --grep @usage",
    "test:changed": "vitest run --changed HEAD~1",
    "test:agents": "vitest run --config vitest.config.agent.ts",
    "test:agents:structural": "vitest run --config vitest.config.agent.ts agent-tests/agent-structure.test.ts",
    "test:agents:smoke": "vitest run --config vitest.config.agent.ts --reporter=verbose agent-tests/agent-smoke.test.ts",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e"
  },
  "dependencies": {
    "@fontsource-variable/jetbrains-mono": "^5.2.8",
    "@tanstack/react-router": "^1.158.4",
    "@tauri-apps/api": "^2",
    "@tauri-apps/plugin-dialog": "^2.6.0",
    "@tauri-apps/plugin-log": "^2.8.0",
    "@tauri-apps/plugin-opener": "^2",
    "@uiw/react-md-editor": "^4.0.11",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "diff": "^8.0.3",
    "github-markdown-css": "^5.9.0",
    "lucide-react": "^0.563.0",
    "next-themes": "^0.4.6",
    "radix-ui": "^1.4.3",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-markdown": "^10.1.0",
    "rehype-highlight": "^7.0.2",
    "remark-gfm": "^4.0.1",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "zustand": "^5.0.11"
  },
  "devDependencies": {
    "@playwright/test": "^1.58.2",
    "@tailwindcss/vite": "^4.1.18",
    "@tauri-apps/cli": "^2",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/diff": "^7.0.2",
    "@types/node": "^25.3.0",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.6.0",
    "autoprefixer": "^10.4.24",
    "jsdom": "^28.0.0",
    "tailwindcss": "^4.1.18",
    "typescript": "~5.8.3",
    "vite": "^7.0.4",
    "vitest": "^4.0.18"
  }
}
